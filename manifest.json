{
  "manifest_version": 2,
  "name": "FoodNetwork Ads Skipper",
  "description": "This extension skip ads in FoodNetwork",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["*://watch.foodnetwork.com/*"],
      "js": ["script.js"]
    }
  ],
  "icons": {
    "128": "icon.png"
  }
}